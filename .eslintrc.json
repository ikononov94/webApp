{
    "extends": "airbnb",
    "env" : {
        "browser": true,
        "jest": true
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-underscore-dangle": 0,
        "no-param-reassign": ["error", { "props": false }],
        "class-methods-use-this": 0,
        "consistent-return": 0
    }
}